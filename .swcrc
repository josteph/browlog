{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "target": "es5",
    "loose": true,
    "externalHelpers": false
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true,
  "minify": false,
  "env": {
    "mode": "entry",
    "coreJs": 3
  }
}